snippet cha "chapter{} and label" 
    \chapter{${1:ChaName}} % (fold)
    \label{cha:${2:${1/\\\w+\{(.*?)\}|\\(.)|(\w+)|([^\w\\]+)/${3:/downcase}${4:+_}/g}}}
    $0

    % chapter $2 (end)

snippet sec "section{} and label" 
    \section{${1:SecName}} % (fold)
    \label{sec:${2:${1/\\\w+\{(.*?)\}|\\(.)|(\w+)|([^\w\\]+)/${3:/downcase}${4:+_}/g}}}
    $0

    % section $2 (end)

snippet sub "subsection{} and label" 
    \subsection{${1:SubName}} % (fold)
    \label{sub:${2:${1/\\\w+\{(.*?)\}|\\(.)|(\w+)|([^\w\\]+)/${3:/downcase}${4:+_}/g}}}
    $0

    % subsection $2 (end)

snippet ssub "subsubsection{} and label" 
