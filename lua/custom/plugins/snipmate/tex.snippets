snippet fig "figure and label" b
    \begin{figure}[H]
      \centering
      \includegraphics[angle=0,width=1.0\textwidth]{${1:Filename}}
      \caption{$0}
      \label{fig:${2:${1/\\\w+\{(.*?)\}|\\(.)|(\w+)|([^\w\\]+)/${3:/downcase}${4:+_}/g}}}
    \end{figure}

snippet tabl "add table" b
    \begin{table}[H]
    \caption{${1:Description}}
    \label{tab:${2:${1/\\\w+\{(.*?)\}|\\(.)|(\w+)|([^\w\\]+)/${3:/downcase}${4:+_}/g}}}
    \centering
      \begin{tabular}{+l^c}
      \rowstyle{\itshape}
             Label1 & Label2 \\\\
      Full spectrum & 0.358 \\\\
        Narrow band & 0.343 \\\\
      \end{tabular}
    \end{table}

snippet par "add paragraph" b
    \paragraph{${1:paragraph name}} % (fold)
    \label{par:${2:${1/\\\w+\{(.*?)\}|\\(.)|(\w+)|([^\w\\]+)/${3:/downcase}${4:+_}/g}}}

    $0

    % paragraph $2 (end)
